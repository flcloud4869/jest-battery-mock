    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "author": "NoDocCat <nodoccat@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/NoDocCat/jest-battery-mock#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoDocCat/jest-battery-mock.git"
  },
  "bugs": {
    "url": "https://github.com/NoDocCat/jest-battery-mock/issues"
  },
  "keywords": [
    "jest",
    "jest-mock",
    "jsdom"
  ],
  "packageManager": "pnpm@7.28.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
