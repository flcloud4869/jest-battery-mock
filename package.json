{
  "name": "jest-battery-mock",
  "type": "module",
  "version": "1.2.2",
  "description": "BatteryManager API mock for Jest",
  "scripts": {
    "build": "tsc && tsup",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
